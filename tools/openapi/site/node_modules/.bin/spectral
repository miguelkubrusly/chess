#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/spectral-cli/dist/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/spectral-cli/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/spectral-cli/dist/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/spectral-cli/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules/@stoplight/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@stoplight+spectral-cli@6.15.0/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@stoplight/spectral-cli/dist/index.js" "$@"
else
  exec node  "$basedir/../@stoplight/spectral-cli/dist/index.js" "$@"
fi
