#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/cli/bin/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/cli/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/cli/bin/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/cli/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules/@redocly/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/@redocly+cli@1.34.3_ajv@8.17.1/node_modules:/home/mk/code/chess/api/doc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@redocly/cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../@redocly/cli/bin/cli.js" "$@"
fi
